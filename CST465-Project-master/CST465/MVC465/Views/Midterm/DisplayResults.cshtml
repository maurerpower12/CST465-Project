@model List<TestQuestion>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Display Results";
}

<h1>Test Answer Review</h1>
@for (int i = 0; i < Model.Count; ++i)
{
    <p>
        @(i + 1) .
        Question: @Html.DisplayFor(m => Model[i].Question)<br /> 
        Answer: @Html.DisplayFor(m => Model[i].Answer)
    </p>
    <br /><br />
}